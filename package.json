{
  "name": "browse-buddy",
  "private": true,
  "version": "1.2.0",
  "author": {
    "name": "Charles Lana",
    "email": "charleslanatester@gmail.com"
  },
  "description": "Easily set up test automation interface",
  "homepage": "https://github.com/charleslana/browse-buddy",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write puppeteer/test",
    "kill": "kill -9 $(lsof -ti:5173)",
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder",
    "build-linux": "vue-tsc && vite build && electron-builder -l",
    "build-win": "vue-tsc && vite build && electron-builder -w",
    "build-win-portable": "vue-tsc && vite build && electron-builder -w portable",
    "preview": "vite preview",
    "test": "ts-node -P tsconfig.script.json ./puppeteer/test/script.test.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "bulma": "^1.0.1",
    "electron-store": "^8.2.0",
    "electron-updater": ">=6.3.0-alpha.6",
    "floating-vue": "^5.2.2",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "puppeteer": "^22.10.0",
    "vue": "^3.4.27",
    "vue-draggable-next": "^2.2.1",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "electron": "^30.0.9",
    "electron-builder": "^24.13.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.27.0",
    "pinia": "^2.1.7",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.0.19"
  },
  "main": "dist-electron/main.js"
}
